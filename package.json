{
  "name": "todo-ist",
  "version": "1.0.0",
  "description": "To-do list",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darco2018/todo-list"
  },
  "author": "Dariusz Ustrzycki <ustrzycki@javacoder.cloud>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darco2018/todo-list/issues"
  },
  "homepage": "http://www.javacoder.cloud",
  "main": "webpack.config.js",
  "keywords": [
    "to-do",
    "list",
    "jQuery"
  ],
  "scripts": {
    "stylelint": "stylelint src/**/*.css --fix",
    "willbeprefixed": "autoprefixer --info",
    "postcss": "postcss src/**/*.css  --base src   --dir build-css --verbose",
    "browsers": "browserslist",
    "coverage": "browserslist --coverage=Pl",
    "doiuse": "npm run sass && doiuse --browsers '> 0.2% in Pl, Chrome >= 70, ios_saf >= 11, Safari >= 12, Edge >= 17, not Samsung < 8.2, not dead, not OperaMini all, not ie > 1, not ie_mob > 1' ./src/css/_custom.css",
    "sass": "node-sass ./src/css/_custom.scss ./src/css/_custom.css",
    "kill": "netstat -nlp | grep 8080",
    "stats": "webpack --profile --json > stats.json",
    "lint": "eslint --fix src/",
    "watch": "webpack -d --watch  --display-error-details",
    "fixCss": "node scripts/fixPathInCssBackground.js",
    "dev": "webpack-dashboard -- node index.js",
    "start": "webpack-dev-server --env development",
    "build": "webpack --env production"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.0",
    "acorn": "^6.1.1",
    "apache-server-configs": "^3.1.0",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "del": "^4.1.0",
    "doiuse": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "main.css": "1.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "normalize.css": "8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.16",
    "postcss-cli": "^6.1.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "1.17.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "ssri": "^6.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.20.2",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.2",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 0.2% in Pl",
    "Chrome >= 70",
    "ios_saf >= 11",
    "Safari >= 12",
    "Edge >= 17",
    "not Samsung < 8.2",
    "not OperaMini all",
    "not ie > 1",
    "not ie_mob > 1",
    "not dead"
  ],
  "h5bp-configs": {
    "directories": {
      "archive": "archive",
      "dist": "dist",
      "src": "src",
      "test": "test"
    }
  },
  "files": [
    "LICENSE.txt",
    "package.json",
    "dist/",
    "README.md"
  ]
}
